{
  "name": "@qualweb/util",
  "version": "0.5.3",
  "description": "Utilities module for qualweb",
  "main": "dist/util.bundle.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --require esm",
    "test:ANameTest": "mocha --require esm test/ANameTest.spec.js",
    "test:ANameTestWiki": "mocha --require esm test/ANameTestWiki.spec.js",
    "test:svg": "mocha --require esm test/ANameSVGTest.spec.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prebuild": "npm run format && rimraf prebuild && tsc --build",
    "build": "rimraf dist && npm run prebuild && webpack --mode production",
    "docs": "typedoc --out docs src/index.ts",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/qualweb/util#readme",
  "bugs": {
    "url": "https://github.com/qualweb/util/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/util.git"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "util",
    "lib"
  ],
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/dom": "^0.2.0",
    "@qualweb/types": "^0.5.6",
    "@tsconfig/recommended": "^1.0.1",
    "@types/lodash.clone": "^4.5.6",
    "@types/node": "^14.14.37",
    "@types/string-pixel-width": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.6.0",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "prettier": "^2.1.2",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "languagedetect": "^2.0.0",
    "lodash.clone": "^4.5.0",
    "string-pixel-width": "^1.10.0"
  }
}
